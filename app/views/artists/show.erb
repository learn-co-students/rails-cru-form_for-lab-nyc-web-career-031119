<h1>My Artist</h1>
<h1>My artist name</h1>
<h3><%= @artist.name %></h1>

<p><%= @artist.bio %></p>

<% @artist.songs.each do |song| %>
  <%= link_to song.name, song_path(song) %>
<% end %>
<br>
<br>
<p><%= link_to "Change your artist", edit_artist_path %></p>
<p><%= link_to "See all artists", artists_path %></p>
